{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2022",
        "outDir": "out",
        "lib": [
            "es2022"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "node_modules",
        "lib"
    ]
}
